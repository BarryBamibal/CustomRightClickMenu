{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"triple-equals": true,
		"no-unreachable": true,
		"forin": true,
		"label-position": true,
		"no-unsafe-finally": true,
		"no-trailing-whitespace": true
	}
}